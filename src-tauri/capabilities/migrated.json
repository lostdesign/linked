{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    {
      "identifier": "fs:write-all",
      "allow": [
        {
          "path": "$DOCUMENT/.linked"
        },
        {
          "path": "$DOCUMENT/.linked/**"
        }
      ]
    },
    {
      "identifier": "fs:read-all",
      "allow": [
        {
          "path": "$DOCUMENT/.linked"
        },
        {
          "path": "$DOCUMENT/.linked/**"
        }
      ]
    },
    "store:default",
    "global-shortcut:default",
    "global-shortcut:allow-register-all"
  ]
}