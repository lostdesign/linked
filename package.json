    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^9",
    "babel-eslint": "10.1.0",
    "dotenv": "16.0.0",
    "electron": "17.0.0",
    "electron-builder": "23.0.2",
    "electron-devtools-installer": "3.1.1",
