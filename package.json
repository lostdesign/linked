  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "__static": "readable",
      "mainStorage": "writable"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "journal",
    "calendar"
  ],
